<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.back.dao.TableNumDao">

    <!-- 查询（列表查询） -->
    <select id="findAllTables" resultType="java.util.List" parameterType="Map">
        SELECT
        TABLE_NAME
        FROM all_tables
        WHERE 1 = 1
    </select>


</mapper>